{
    "PolicyName": "root()()",
    "PolicyDocument": {
        "Version": "2012-10-17",
        "Statement": [
            {
                "Sid": "IamSpecificAccess",
                "Effect": "Allow",
                "Action": [
                    "iam:ListRoles",
                    "iam:ListUsers",
                    "iam:CreateRole"
                ],
                "Resource": "arn:aws:iam::123456789012:role/role-name"
            }
        ]
    }
}
